{
    "version": 2,
    "builds": [
      {
        "src": "dist/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "dist/**/*",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      {
        "source": "/:path*",
        "destination": "/index.html"
      }
    ]
  }
  